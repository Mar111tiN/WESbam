# ################# FILTER ###################################################
rule filter_variants:
    input: "table/{sample}_{tumor}-{norm}.raw.csv"
    output: "filter/{sample}_{tumor}-{norm}.{filter}.csv"
    wildcard_constraints:
        sample = "[^_]+"
    conda:
        "../env/filter-env.yml"
    threads:
        config['filter']['threads']
    script:
        "../scripts/filter.py"